[
  {
    "name": "resume_app", 
    "image": "carlosnunez/resume_app:fake_version",
    "networkMode": "awsvpc",
    "portMappings": [
      {
        "containerPort": 4567, 
        "hostPort": 4567, 
        "protocol": "tcp"
      }
    ],
    "essential": true, 
    "environment": [
      {
        "name": "S3_BUCKET_NAME", 
        "value": "change me"
      },
      {
        "name": "RESUME_NAME",
        "value": "latest"
      },
      {
        "name": "AWS_ACCESS_KEY_ID",
        "value": "fake"
      },
      {
        "name": "AWS_SECRET_ACCESS_KEY",
        "value": "fake"
      },
      {
        "name": "AWS_REGION",
        "value": "us-east-1"
      }
    ], 
    "readonlyRootFilesystem": true, 
    "logConfiguration": {
      "logDriver": "awslogs", 
      "options": {
        "awslogs-region": "us-east-1",
        "awslogs-group": "resume_app_logs",
        "awslogs-stream-prefix": "resume-app"
      }
    }
  }
] 
