[
  {
    "name": "resume_app", 
    "image": "carlosonunez/resume_app:fake_version", 
    "portMappings": [
      {
        "containerPort": 4567, 
        "hostPort": 80, 
        "protocol": "tcp"
      }
    ],
    "essential": true, 
    "environment": [
      {
        "name": "S3_BUCKET_NAME", 
        "value": "i_am_a_bucket"
      },
      {
        "name": "RESUME_NAME",
        "value": "latest"
      }
    ], 
    "readonlyRootFilesystem": true, 
    "logConfiguration": {
      "logDriver": "awslogs", 
      "options": {
        "KeyName": ""
      }
    }
  }
] 
