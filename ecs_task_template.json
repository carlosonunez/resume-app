{
  "networkMode": "awsvpc", 
  "containerDefinitions": [
    {
      "name": "resume_app", 
      "image": "carlosonunez/resume_app:$__VERSION__", 
      "portMappings": [
        {
          "containerPort": 4567, 
          "hostPort": 80, 
          "protocol": "tcp"
        }
      ],
      "essential": true, 
      "environment": [
        {
          "name": "S3_BUCKET_NAME", 
          "value": "$__S3_BUCKET_NAME__"
        },
        {
          "name": "RESUME_NAME",
          "value": "$__RESUME_NAME__"
        }
      ], 
      "readonlyRootFilesystem": true, 
      "logConfiguration": {
        "logDriver": "awslogs", 
        "options": {
          "KeyName": ""
        }
      }
    }
  ], 
  "requiresCompatibilities": [
    "FARGATE"
  ], 
  "cpu": "256", 
  "memory": "512"
}
